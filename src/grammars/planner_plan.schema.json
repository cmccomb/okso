{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PlannerPlan",
  "type": "array",
  "items": {
    "type": "string",
    "minLength": 1
  },
  "minItems": 1,
  "description": "Ordered list of high-level plan steps. Each item will be numbered downstream before execution.",
  "examples": [
    [
      "Inspect repository layout",
      "Use final_answer to summarize"
    ],
    [
      "Run tests",
      "Review failures",
      "Use final_answer to report findings"
    ]
  ]
}
